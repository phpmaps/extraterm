{
  "name": "open-link",
  "displayName": "Open Link",
  "version": "1.0.0",
  "description": "",
  "windowMain": "dist/OpenLink.js",
  "scripts": {
    "build": "tsc",
    "clean": "shx rm -rf dist data"
  },
  "author": "",
  "license": "MIT",
  "contributes": {
    "commands": [
      {
        "command": "open-link:openLink",
        "title": "Open Link",
        "category": "hyperlink",
        "order": 800,
        "icon": "fas fa-external-link-alt",
        "when": "isHyperlink && (hyperlinkProtocol == 'http:' || hyperlinkProtocol == 'https:' || hyperlinkProtocol == 'ftp:' || hyperlinkProtocol == 'file:')"
      }
    ],
    "menus": {
      "contextMenu": [
        {
          "command": "open-link:openLink",
          "show": true
        }
      ]
    }

  },
  "devDependencies": {
    "@extraterm/extraterm-extension-api": "0.13.0",
    "shelljs": "0.8.5",
    "typescript": "4.1.3"
  }
}
